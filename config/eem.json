{
    "experiment_details":"to test the dependence of the algorithm on expert prompt",
    "dataset_path": "data/eem_three_retrivals.jsonl",
    "mini_batch_size": 5,
    "batch_size": 7,
    "iterations": 1,
    "epochs": 10,
    "logging_file_path": "logs/eem_changed_prompts.jsonl",
    "epsilon": 0.5,
    "beam_width": 3,
    "group_frequency": 2,
    "papyrus": false,
    "create_sub_prompts": false,
    "num_sub_prompts": 5,
    "cache_path": "cache/eem.db",
    "initial_prompt": "You are a system helping to improve the relevance of search results. You are given the first search query, second search query, and the market. The market indicates the language and the country the queries come from. You have to answer 'Yes' or 'No' to the question. Here are some examples to help you understand the task better.",
    "metric_kwargs": {
        "type": "weighted_accuracy",
        "weights": [0.5,0.5]
    },
    "solver_llm": {
        "model_kwargs": {
            "model": "gpt-4-turbo",
            "temperature": 0
        },
        "api_kwargs": {
            "api_type": "azure",
            "api_base": "https://msri-openai-ifaq.azure-api.net/",
            "api_version": "2023-03-15-preview"
        }
    },
    "expert_llm": {
        "model_kwargs": {
            "model": "gpt-4-turbo",
            "temperature": 0.7
        },
        "api_kwargs": {
            "api_type": "azure",
            "api_base": "https://msri-openai-ifaq.azure-api.net/",
            "api_version": "2023-03-15-preview"
        }
    },
    "grouping_llm": {
        "model_kwargs": {
            "model": "gpt-4-turbo",
            "temperature": 0.7
        },
        "api_kwargs": {
            "api_type": "azure",
            "api_base": "https://msri-openai-ifaq.azure-api.net/",
            "api_version": "2023-03-15-preview"
        }
    }
}